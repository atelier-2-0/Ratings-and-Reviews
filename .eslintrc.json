{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["babel.config.js"],
    "rules": {
      "no-console": "off",
      "no-unused-vars":"warn",
      "max-len": [
        "error", {"code": 80},
        {"ignoreComments": true}
      ],
      "arrow-body-style": ["error", "as-needed"]
    }
}
